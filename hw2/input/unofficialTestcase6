correct;

begin 
    var a: array 1 to 2 of real;
    var a: array 1 to 2 of array 1 to 2 of array 3 to 5 of real;
    var c, d : integer;
    a[10][11][22][33][555][111][a+a] := 1;
    a[0010][11][22][33][555][111][a+a] := 1;
    a[-10][11][22][33][555][111][a+a] := 1;
end

end correct
